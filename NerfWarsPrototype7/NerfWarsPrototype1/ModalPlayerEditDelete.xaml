<Window x:Class="NerfWarsPrototype1.ModalPlayerEditDelete"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="EditDeletePlayerModal" Height="200" Width="339" ResizeMode="NoResize" WindowStartupLocation="CenterOwner">
    <Grid>
        <TextBlock x:Name="tblSearchAPlayer" Margin="10,10,0,0" TextWrapping="Wrap" Text="Search for a player to edit" FontSize="15" VerticalAlignment="Top"/>
        <Label Margin="10,47,0,0" Content="Last name" HorizontalAlignment="Left" VerticalAlignment="Top" />

        <TextBox Name="tbLastName" HorizontalAlignment="Left" Height="23" Margin="94,47,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="151"/>
        <ComboBox HorizontalAlignment="Left" Text="test" Margin="10,88,0,0" VerticalAlignment="Top" Width="313"/>
        <Button x:Name="btnPlayerModalEdit" Content="Edit" HorizontalAlignment="Left" Margin="43,140,0,0" VerticalAlignment="Top" Width="75" Click="btnPlayerModalEdit_Click"/>
        <Button x:Name="btnPlayerModalCancel" Content="Cancel" HorizontalAlignment="Left" Margin="196,140,0,0" VerticalAlignment="Top" Width="75"/>

    </Grid>
</Window>
