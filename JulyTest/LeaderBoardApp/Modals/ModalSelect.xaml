<Window x:Class="LeaderBoardApp.Modals.ModalSelect"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="EditDeletePlayerModal" Height="175" Width="335"
        WindowStyle="None" ResizeMode="NoResize" WindowStartupLocation="CenterOwner">
    <Grid>
        <TextBlock x:Name="tblSearchAPlayerorTeam" Margin="10,10,0,0" TextWrapping="Wrap" FontSize="15" VerticalAlignment="Top"/>
        <Label x:Name="lblSearch" Margin="10,47,0,0" Content="" HorizontalAlignment="Left" VerticalAlignment="Top" />

        <TextBox x:Name="tbSearch" HorizontalAlignment="Left" Height="23" Margin="94,47,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="151" />
        <ComboBox x:Name="cmbNames" HorizontalAlignment="Left" Text="test" Margin="10,88,0,0" VerticalAlignment="Top" Width="313"/>
        <Button x:Name="btnModalEditDelete" Content="Edit" HorizontalAlignment="Left" Margin="43,140,0,0" VerticalAlignment="Top" Width="75" />
        <Button x:Name="btnModalDone" Content="Done" HorizontalAlignment="Left" Margin="196,140,0,0" VerticalAlignment="Top" Width="75" Click="btnPlayerModalDone_Click"/>

    </Grid>
</Window>
